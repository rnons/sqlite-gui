{
  "name": "sqlite-gui",
  "version": "1.0.0",
  "description": "a toy gui for sqlite",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "dev": "watchify -t [ babelify --optional es7.decorators ] -o dist/app.js src/app.js & postcss -c postcss.conf.js --local-plugins -o dist/main.css src/styles/main.css -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rnons/sqlite-gui.git"
  },
  "keywords": [
    "sqlite",
